/**app.wxss**/
@import "./behaviors/animation/animation.wxss";

.container {
  display: flex;
  flex-direction: column;
  align-items: center;
  box-sizing: border-box;
} 

button {
  background: initial;
}

button:focus{
  outline: 0;
}

button::after{
  border: none;
}


page {
    background-color: #d7faf4;
    display: flex;
    flex-direction: column;
    justify-content: flex-start;
    --BorderColor: blueviolet;
    --Pagebg_light: #d7faf4
    --Pagebg_dark: #262626
}